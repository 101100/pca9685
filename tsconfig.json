{
  "compilerOptions": {
    "declaration": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "es5",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "outDir": "dist",
    "removeComments": false,
    "sourceMap": false,
    "target": "es5"
  },
  "exclude": [
    "dist",
    "node_modules",
    "typings/globals",
    "typings/modules"
  ]
}
